   "source": [
    "N = int(input())\n",
    "dp = [0]*(N+1)\n",
    "for _ in range(2, N+1):\n",
    "    dp[_] = dp[_-1]+1\n",
    "    if _%2 == 0:\n",
    "        dp[_] = min(dp[_], dp[_//2]+1)\n",
    "    if _%3 == 0:\n",
    "        dp[_] = min(dp[_], dp[_//3]+1)\n",
    "print(dp[N])"
   ]
